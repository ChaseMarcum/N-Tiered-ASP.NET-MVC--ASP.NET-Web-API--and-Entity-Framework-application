@{
    ViewBag.Title = "Home Page";
    var ActiveUserName = TempData["ActiveUserName"];
    TempData["ActiveUserName"] = ActiveUserName;
    var ActivePermissions = TempData["ActivePermissions"];
    TempData["ActivePermissions"] = ActivePermissions;
}

<div>
    <ul class="breadcrumb">
        <li>
            @Html.ActionLink("Home", "Index", "Home") <span class="divider">/</span>
        </li>
        <li>
            @Html.ActionLink("Dashboard", "Index", "Home")
        </li>
    </ul>
</div>

<div class="sortable row-fluid">
    <a data-rel="tooltip" title="6 new members." class="well span3 top-block" href="#">
        <span class="icon32 icon-red icon-users"></span>
        <div>Total Applicants</div>
        <div>54</div>
        <span class="notification">6</span>
    </a>
    <a data-rel="tooltip" title="4 new pro members." class="well span3 top-block" href="#">
        <span class="icon32 icon-orange icon-user"></span>
        <div>Qualified Applicants</div>
        <div>12</div>
        <span class="notification green">4</span>
    </a>
    <a data-rel="tooltip" title="2 new members." class="well span3 top-block" href="#">
        <span class="icon32 icon-green icon-user"></span>
        <div>Recommended Applicants</div>
        <div>2</div>
        <span class="notification yellow">1</span>
    </a>

    <a data-rel="tooltip" title="1 new members." class="well span3 top-block" href="#">
        <span class="icon32 icon-blue icon-user"></span>
        <div>Background Needed</div>
        <div>5</div>
        <span class="notification red">12</span>
    </a>
</div>

<div class="row-fluid">
    <div class="box span12">
        <div class="box-header well">
            <h2><i class="icon-info-sign"></i> WELCOME @ActiveUserName</h2>
            <div class="box-icon">
                <a href="#" class="btn btn-setting btn-round"><i class="icon-cog"></i></a>
                <a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
                <a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
            </div>
        </div>
        <div class="box-content">
            <div class="img-responsive">
                <img src="~/img/AIMTitlePageLogo.png" />
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>


